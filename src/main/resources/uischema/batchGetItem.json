      "value": {
        "groupName": "General",
        "elements": [
          {
            "type": "attribute",
            "value": {
              "name": "configRef",
              "displayName": "Connection Name",
              "inputType": "connection",
              "allowedConnectionTypes": [
                "AWSBasic"
              ],
              "defaultValue": "",
              "required": "true",
              "helpTip": "Connection name"
            }
          }
        ]
      }
    },
    {
      "type": "attributeGroup",
      "value": {
        "groupName": "Basic",
        "elements": [
          {
